    "eslint-plugin-react-hooks": "~5.0.0",
    "globals": "^15.15.0",
    "html-loader": "^5.1.0",
    "immer": "^10.1.1",
    "peggy": "^4.2.0",
    "prettier": "^3.5.1",
    "prettier-plugin-pegjs": "^2.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "text-loader": "^0.0.1",
