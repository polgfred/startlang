    "@eslint/js": "^9.20.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/material": "^6.4.4",
    "@mui/material-pigment-css": "^6.4.3",
    "@mui/system": "^6.4.3",
    "@pigment-css/nextjs-plugin": "^0.0.30",
    "@pigment-css/react": "^0.0.29",
    "@types/deep-equal": "^1.0.4",
    "blockly": "^11.2.1",
    "classnames": "^2.5.1",
    "deep-equal": "^2.2.3",
