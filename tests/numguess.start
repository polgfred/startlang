let number = rand(1, 100)

repeat 10 do
  let guess = num(input("Guess a number between 1 and 100: "))
  
  if guess = number then
    print "You got it!"
    exit
  else
    if guess < number then
      print "Too low."
    else
      print "Too high."
    end
  end
end

print "Sorry, out of guesses."
