upper = 1000
check = sqrt(upper)
primes = list(upper)

for i in 0,1 do primes[i] = false
for i in 2,upper-1 do primes[i] = true

i = 0
while i <= check do
  while i <= check and not primes[i] do i = i+1
  for j in i*2,upper-1,i do primes[j] = false
  i = i+1
end

for i in 0,upper-1 do
  if primes[i] then print "`i` is prime"
end
