let primes = array(1000)

for i in step(0, 2) do let primes[i] = 0
for i in step(2, 1000) do let primes[i] = 1

let i = 0
while i < 1000 do
  while i < 1000 and primes[i] = 0 do let i = i + 1
  for j in step(i * 2, 1000, i) do let primes[j] = 0
  let i = i + 1
end

for i in step(0, 1000) do
  if primes[i] = 1 then print i + " is prime"
end
