colors = list("black",
              "blue",
              "red",
              "green",
              "orange",
              "yellow")

box = rect(0, 0, 10, 10)

count t from 0 to 72 do
  with box do
    move t*5, t*5
    rotate t*5
    scale t/2
    fill colors[floor(t/6) % len(colors)]
  end
  sleep 0.05
end
