let colors = list()
push colors,"black",
            "blue",
            "red",
            "green",
            "orange",
            "yellow"

let box = rect(0,0,10,10)

for t in range(0,72) do
  move box,t*5,t*5
  rotate box,t*5
  scale box,t/2
  fill box,colors[floor(t/6)%len(colors)]
  sleep 0.05
end
