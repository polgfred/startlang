begin compare(first, second) do
  if first > second then
    return 1
  else if first < second then
    return -1
  else
    return 0
  end
end

for x from 8 to 13 do
  print "compare(`x`, 10) is `compare(x, 10)`"
end

begin func1(i) do
  while i < 60 do i = i * 2
  return i
end

begin func2(i) do
  j = i
  while j < func1(i) do j = j + 11
  return j
end

print "func2(10) = `func2(10)` = 87"
