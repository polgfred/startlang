opacity 1/4
stroke "white"

repeat 400 do
  red = rand()
  green = rand()
  blue = rand()
  rgb = color(red, green, blue)
  fill rgb

  rot = rand(-20, 20)
  rotate rot

  x = rand(0, 650)
  y = rand(0, 500)

  shape = rand(0, 1)
  if shape = 1 then
    w = rand(50, 100)
    h = rand(50, 100)
    rect x, y, w, h
  else
    r = rand(25, 50)
    circle x, y, r
  end
end
